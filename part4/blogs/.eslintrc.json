{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "import/extensions": 0,
    "consistent-return": 0,
    "import/no-extraneous-dependencies": 0,
    "operator-linebreak": 0,
    "no-underscore-dangle": 0,
    "comma-dangle": 0,
    "no-plusplus": 0
  }
}
